pipeline {
    agent any

    stages {
        stage('Clone') {
            steps {
                git 'https://github.com/Lesterm14/Jenkins_pipeline.git'
            }
        }
        stage('Test') {
            steps {
                echo 'Running unit tests...'
                // Simulate test script
                sh 'echo All tests passed.'
            }
        }
    }
}
