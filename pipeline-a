pipeline {
    agent any

    stages {
        stage('Clone') {
            steps {
                git 'https://github.com/Lesterm14/Jenkins_pipeline.git'
            }
        }
        stage('Build') {
            steps {
                echo 'Building the application...'
                // Example build command
                sh 'echo Compiling code...'
            }
        }
    }
}
